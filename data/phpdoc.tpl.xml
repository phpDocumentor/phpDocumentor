<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor>
    <parser>
        <default-package-name>Default</default-package-name>
        <encoding>utf-8</encoding>
        <target>output/build</target>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
            <extension>php3</extension>
            <extension>phtml</extension>
        </extensions>
        <visibility></visibility>
    </parser>
    <files>
        <ignore-hidden>on</ignore-hidden>
        <ignore-symlinks>on</ignore-symlinks>
    </files>
    <transformer>
        <target>output</target>
    </transformer>
    <logging>
        <level>info</level>
        <paths>
            <default>phpdoc-{DATE}.log</default>
            <errors>phpdoc-{DATE}.errors.log</errors>
        </paths>
    </logging>
    <transformations>
        <template name="clean"/>
    </transformations>
</phpdocumentor>
