    <link rel="stylesheet" href="{{ asset('css/badge_only.css') }}" type="text/css" />

    {# Keep modernizr in head - http://modernizr.com/docs/#installing #}
    <script src="{{ asset('js/modernizr.min.js') }}"></script>

