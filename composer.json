{
    "name": "phpdocumentor/phpdocumentor",
    "description": "Documentation Generator for PHP",
    "keywords": ["documentation", "api", "dga", "application", "phpdoc"],
    "homepage": "http://www.phpdoc.org",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "phpDocumentor\\": [ "src/phpDocumentor/" ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "phpDocumentor\\": [
                "tests/unit/phpDocumentor/",
                "tests/integration/phpDocumentor/"
            ],
            "phpDocumentor\\Behat\\Contexts\\": [
                "tests/features/bootstrap"
            ]
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "ext-zlib": "*",
        "erusev/parsedown": "~1.0",
        "jean85/pretty-package-versions": "^1.2",
        "league/flysystem": "^1.0",
        "league/pipeline": "^1.0",
        "ocramius/package-versions": "^1.4",
        "padraic/phar-updater": "^1.0",
        "phpdocumentor/flyfinder": "1.*@beta",
        "phpdocumentor/graphviz": "2.x-dev@dev",
        "phpdocumentor/reflection": "4.*@alpha",
        "phpdocumentor/reflection-common": "2.*@alpha",
        "phpdocumentor/reflection-docblock": "5.*@alpha",
        "phpdocumentor/type-resolver": "^0.7.1",
        "psr/cache": "^1.0",
        "psr/log": "^1.1",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/event-dispatcher": "5.0.*",
        "symfony/expression-language": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/stopwatch": "^5.0",
        "symfony/yaml": "^5.0",
        "tedivm/stash": "^0.14.2",
        "twig/twig": "~2.0",
        "webmozart/assert": "^1.3",
        "zendframework/zend-serializer": "~2.1",
        "zendframework/zend-stdlib": "~2.1"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "mikey179/vfsstream": "^1.2",
        "mockery/mockery": "^1.0"
    },
    "suggest": {
        "ext-twig": "Enabling the twig extension improves the generation of twig based templates.",
        "ext-ctype": "For best performance"
    },
    "config": {
        "bin-dir":"bin/",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "bin": ["bin/phpdoc.php", "bin/phpdoc"],
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "symfony": {
            "id": "01C32VS9393M1CP9R8TEJMH62G",
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
